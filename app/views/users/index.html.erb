<h1>Users</h1>
<% if @users.length.positive? %>
  <table>
    <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Domain</th>
      <th>Email</th>
    </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.domain %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
<% else %>
  <p>No Users found</p>
<% end %>
